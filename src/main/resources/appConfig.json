{
	"http_protocol": "http://",
	"http_server_host": "localhost",
	"http_server_port": 8080,
	"context_path": "/runner/api",
	"verticle_package_name": "com.greyseal.vertx.boot.verticle",
	"handler_package_name": "com.greyseal.vertx.boot.handler",
	"http2Enabled": false,
	"dev": {
		"httpServerVerticle": {
			"ha": false,
			"instances": 1,
			"maxWorkerExecuteTime": 60000000000,
			"multiThreaded": false,
			"worker": false,
			"workerPoolSize": 200
		},
		"http_server_options": {
			"acceptUnmaskedFrames": false,
			"alpnVersions": [
				"HTTP_2",
				"HTTP_1_1"
			],
			"compressionLevel": 6,
			"compressionSupported": true,
			"decompressionSupported": false,
			"handle100ContinueAutomatically": false,
			"http2ConnectionWindowSize": -1,
			"initialSettings": {
				"headerTableSize": 4096,
				"initialWindowSize": 65535,
				"maxConcurrentStreams": 100,
				"maxFrameSize": 16384,
				"maxHeaderListSize": 2147483647,
				"pushEnabled": true
			},
			"maxChunkSize": 8192,
			"maxHeaderSize": 8192,
			"maxInitialLineLength": 4096,
			"maxWebsocketFrameSize": 65536,
			"maxWebsocketMessageSize": 262144
		}
	}
}